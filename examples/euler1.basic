
0000 REM Count multiples of 3 and 5 below 1000

0010 LET I = 3
0011 LET SUM = 0

9000 PRINT SUM
9001 END

0100 LET TEMP = I % 3
0110 IF TEMP = 0 THEN 400

0200 LET TEMP = I % 5
0210 IF TEMP = 0 THEN 400

0300 LET I = I + 1
0310 IF I = 1000 THEN 9000
0320 GOTO 100

0400 LET SUM = SUM + I
0410 GOTO 300